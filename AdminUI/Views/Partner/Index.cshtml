@model IEnumerable<AdminUI.Models.PartnerViewModel>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AssociateName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AssociateUrl)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AssociateAddress)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AssociateName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AssociateUrl)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AssociateAddress)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                </td>
            </tr>
        }
    </tbody>
</table>
